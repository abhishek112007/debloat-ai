{
  "name": "debloat-ai",
  "version": "1.0.0",
  "description": "Debloat AI - Remove bloatware safely from Android devices",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run frontend\" \"npm run tauri\" --names \"VITE,TAURI\" --prefix-colors \"cyan,magenta\"",
    "frontend": "cd frontend && vite",
    "build": "cd frontend && tsc && vite build",
    "preview": "cd frontend && vite preview",
    "tauri": "tauri dev",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build"
  },
  "dependencies": {
    "@tanstack/react-virtual": "^3.13.12",
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-dialog": "^2.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.25",
    "lucide-react": "^0.555.0",
    "react": "^18.2.0",
    "react-chatbotify": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0",
    "@types/node": "^24.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.1",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  }
}
