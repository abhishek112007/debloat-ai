{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "Android ADB Manager",
  "version": "0.1.0",
  "identifier": "com.adb.manager",
  "build": {
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "./frontend/dist"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "backend/tauri/icons/32x32.png",
      "backend/tauri/icons/128x128.png",
      "backend/tauri/icons/128x128@2x.png",
      "backend/tauri/icons/icon.icns",
      "backend/tauri/icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "longDescription": "Android device manager using ADB",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "linux": {
      "deb": {
        "depends": []
      }
    },
    "externalBin": []
  },
  "app": {
    "windows": [
      {
        "title": "Android ADB Manager",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "transparent": false
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": ["**"]
      }
    }
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}
