{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "Debloat AI",
  "version": "1.0.0",
  "identifier": "com.debloat.ai",
  "build": {
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "./frontend/dist"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "backend/tauri/icons/32x32.png",
      "backend/tauri/icons/128x128.png",
      "backend/tauri/icons/128x128@2x.png",
      "backend/tauri/icons/icon.icns",
      "backend/tauri/icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "",
      "nsis": {
        "installMode": "currentUser",
        "languages": [
          "en-US"
        ]
      }
    },
    "shortDescription": "AI-Powered Android Debloater",
    "longDescription": "Safely remove bloatware from your Android devices using ADB and AI intelligence. Get intelligent recommendations about which packages are safe to remove.",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "linux": {
      "deb": {
        "depends": []
      }
    },
    "externalBin": []
  },
  "app": {
    "windows": [
      {
        "title": "Debloat AI",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "transparent": false
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": [
          "**"
        ]
      }
    }
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}